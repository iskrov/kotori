{
  "expo": {
    "name": "Vibes",
    "slug": "vibes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibes.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vibes.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#ffffff",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons", "react-native-gesture-handler", "react-native-reanimated"]
        }
      },
      "bundler": "webpack",
      "output": "static",
      "themeColor": "#7D4CDB"
    },
    "extra": {
      "apiUrl": "http://localhost:8001",
      "googleClientId": "your-google-client-id-here",
      "googleCloudProjectId": "gen-lang-client-0845999334",
      "eas": {
        "projectId": "your-project-id"
      }
    }
  },
  "name": "vibes"
} 