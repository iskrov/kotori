    "eslint-plugin-react-native": "^5.0.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.5.0",
    "jest-expo": "~48.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^4.9.4",
    "unimported": "^1.31.1",
    "url": "^0.11.4"
  },
  "private": true
}
