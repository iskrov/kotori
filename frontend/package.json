{
  "name": "kotori-frontend",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "test:integration": "jest --testPathPattern=e2e --testNamePattern=\"Integration Tests\" --detectOpenHandles",
    "test:e2e": "jest --testPathPattern=e2e --detectOpenHandles",
    "test:integration:cross-platform": "jest --testPathPattern=CrossPlatform --detectOpenHandles",
    "test:performance": "jest --testPathPattern=performance --detectOpenHandles",
    "test:security": "jest --testPathPattern=security --detectOpenHandles",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:ios": "detox test --configuration ios.sim.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "e2e:workflow": "detox test e2e/__tests__/shareWorkflow.e2e.ts",
    "e2e:errors": "detox test e2e/__tests__/errorScenarios.e2e.ts",
    "e2e:performance": "detox test e2e/__tests__/performance.e2e.ts",
    "e2e:accessibility": "detox test e2e/__tests__/accessibility.e2e.ts",
    "e2e:all": "detox test e2e/__tests__/",
    "test-speech-api": "node scripts/test-speech-api.js",
    "simple-api-test": "node scripts/simple-api-test.js",
    "check-api": "node scripts/check-speech-api.js",
    "speech-example": "node scripts/speech-api-example.js",
    "opaque:smoke": "node scripts/opaque-smoke.js",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "analyze:dead-code": "ts-unused-exports tsconfig.json",
    "analyze:unused-imports": "unimported",
    "quality:check": "npm run lint && npm run analyze:dead-code && npm run analyze:unused-imports",
    "quality:fix": "npm run lint:fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
